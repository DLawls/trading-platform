{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OHLCV Data",
  "type": "object",
  "properties": {
    "ticker": {"type": "string"},
    "datetime": {"type": "string", "format": "date-time"},
    "open": {"type": "number"},
    "high": {"type": "number"},
    "low": {"type": "number"},
    "close": {"type": "number"},
    "volume": {"type": "number"}
  },
  "required": ["ticker", "datetime", "open", "high", "low", "close", "volume"]
} 